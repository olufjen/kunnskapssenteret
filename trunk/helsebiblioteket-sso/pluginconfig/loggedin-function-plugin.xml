<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <!-- Configure the function library to respond at extension "example". -->
    <bean class="com.enonic.cms.api.plugin.FunctionLibraryPlugin">
        <property name="name" value="loggedinuser"/>
        <property name="target">
            <bean class="no.helsebiblioteket.evs.plugin.LoggedInFunction"/>
        </property>
    </bean>
</beans>
