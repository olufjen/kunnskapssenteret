<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean class="no.helsebiblioteket.admin.sqlmapdao.plugin.IbatisProofOfConceptPlugin">
        <property name="urlPattern" value="/site/[0-3]/ibatis"/>
        <property name="priority" value="1"/>
        <!-- The displayName property is new in 4.4.2.  Do not set this in 4.4.0 or 4.4.1.  -->
        <property name="displayName" value="helsebiblioteket sqlmap dao plugin 2."/>
        <property name="personDao" ref="personDao"/>
        
    </bean>

    <bean id="personDao" class="no.helsebiblioteket.admin.sqlmapdao.SqlMapPersonDao"/>
 
</beans>