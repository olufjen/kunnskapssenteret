<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="ProxyConfig">
	<typeAlias alias="ProxyConfigLine" type="no.helsebiblioteket.admin.domain.line.ProxyConfigLine" />
	<resultMap id="ProxyConfigLineMap" class="ProxyConfigLine">
		<result property="id" column="config_id" />
		<result property="groupName" column="groupname" />
		<result property="property" column="property" />
		<result property="propertyValue" column="property_value" />
		<result property="lastChanged" column="last_changed" />
	</resultMap>
	<select id="getProxyConfigListAll" resultMap="ProxyConfigLineMap">
		select config_id, groupname, property, property_value, date_trunc('milliseconds', last_changed) as last_changed
		from tbl_config
		order by groupname, property, property_value;
	</select>
</sqlMap>
